{
  "manifest_version": 3,
  "name": "Canvas Today",
  "version": "0.1",
  "icons": {
    "16": "canvas-today-16.png",
    "48": "canvas-today-48.png",
    "128": "canvas-today-128.png"
  },
  "description": "Automatically scrolls Canvas home pages with course schedule format to current session",
  "content_scripts": [
    {
      "matches": ["https://dartmouth.instructure.com/*","https://canvas.dartmouth.edu/*"],
      "js": ["jquery-3.5.0.min.js", "content.js"]
    }
  ],
  "permissions": ["tabs"]
}
